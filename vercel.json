{
    "version": 2,
    "builds": [{ "src": "server.js", "use": "@vercel/node" }],
    "routes": [
        { "src": "/api/(.*)", "dest": "server.js" },
        { "src": "/webhook", "dest": "server.js" }
    ],
    "crons": [
        {
            "path": "/api/cron/process-payments",
            "schedule": "15 7 * * *"
        }
    ]
}